{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/npm-debug.log*": true,
        "**/yarn-debug.log*": true,
        "**/yarn-error.log*": true,
        "**/lerna-debug.log*": true,
        "**/.npm": true,
        "**/.eslintcache": true,
        "**/.stylelintcache": true,
        "**/*.tsbuildinfo": true,
        "**/.vscode-test": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/dist": true,
        "**/build": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/build/**": true,
        "**/.npm/**": true,
        "**/.eslintcache": true,
        "**/.stylelintcache": true
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "editor.renderWhitespace": "selection",
    "editor.renderControlCharacters": false,
    "editor.renderLineHighlight": "all",
    "editor.rulers": [80, 120],
    "editor.wordWrap": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": true,
    "editor.minimap.maxColumn": 120,
    "editor.suggestSelection": "first",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.parameterHints.enabled": true,
    "editor.hover.enabled": true,
    "editor.definitionLinkOpened": false,
    "debug.console.fontSize": 14,
    "debug.console.lineHeight": 1.4,
    "debug.console.wordWrap": true,
    "debug.console.historySuggestions": true,
    "debug.terminal.clearBeforeReusing": true,
    "debug.node.autoAttach": "on",
    "debug.node.terminalOptions": {
        "cwd": "${workspaceFolder}/backend"
    },
    "debug.allowBreakpointsEverywhere": true,
    "debug.inlineValues": "on",
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.showInlineBreakpointCandidates": true,
    "debug.toolBarLocation": "docked",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.4,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.showExitAlert": true,
    "terminal.integrated.shell.windows": "cmd.exe",
    "npm.enableScriptExplorer": true,
    "npm.scriptExplorerAction": "run",
    "npm.includeDirectory": "backend",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggest.autoImports": true,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.validate.enable": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.suggest.autoImports": true,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.validate.enable": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "vue": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "html.autoClosingTags": true,
    "css.autoClosingTags": true,
    "javascript.autoClosingTags": true,
    "typescript.autoClosingTags": true,
    "files.associations": {
        "*.js": "javascript",
        "*.json": "json",
        "*.html": "html",
        "*.css": "css",
        "*.md": "markdown"
    },
    "emmet.syntaxProfiles": {
        "javascript": "jsx",
        "vue": "html"
    },
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.showInlineOpenFileAction": false,
    "git.suggestSmartCommit": false,
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "vs-seti",
    "workbench.editor.enablePreview": true,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.tabCloseButton": "right",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": false,
    "workbench.panel.defaultLocation": "bottom",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.enabled": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "problems.showCurrentInStatus": true,
    "problems.decorations.enabled": true,
    "task.quickOpen.history": 0,
    "task.allowAutomaticTasks": "on",
    "launch.configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Backend",
            "program": "${workspaceFolder}/backend/server.js",
            "cwd": "${workspaceFolder}/backend",
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal",
            "restart": true,
            "runtimeExecutable": "nodemon"
        }
    ],
    "workbench.editor.tabActionLocation": "right",
    "editor.guides.indentation": true
}